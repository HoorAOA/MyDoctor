<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/accent_color</item>
        <item name="colorAccent">@color/accent_color</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="textAppearanceSubtitle1">@style/TextAppearance.App.Subtitle1</item>
        <item name="textAppearanceCaption">@style/TextAppearance.App.Caption</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

    <style name="CropperTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/accent_color</item>
        <item name="colorPrimaryDark">@color/accent_color</item>
        <item name="colorAccent">@color/accent_color</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <declare-styleable name="EditTextWithErrorView">
        <attr name="hint" format="string" />
        <attr name="error_text" format="string" />
        <attr name="show_error_label" format="boolean" />
        <attr name="android:inputType" />

    </declare-styleable>


